{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "jsdoc-format": false,
    "forin": false,
    "no-console": false,
    "no-unused-variable": true,
    "no-shadowed-variable": false,
    "one-variable-per-declaration": false,
    "object-literal-sort-keys": false,
    "variable-name": false
  },
  "rulesDirectory": []
}
